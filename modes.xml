  <mode name="pl-uk" install="yes">
    <pipeline>
      <program name="lt-proc">
        <file name="pl-uk.automorf.bin"/>
      </program>
      <program name="apertium-tagger -g $2">
        <file name="pl-uk.prob"/>
      </program>
      <program name="apertium-pretransfer"/>
