        <file name="pl-uk.autobil.bin"/>
      </program>
      <program name="apertium-interchunk">
        <file name="apertium-pl-uk.pl-uk.t2x"/>
        <file name="pl-uk.t2x.bin"/>
      </program>
      <program name="apertium-postchunk">
        <file name="apertium-pl-uk.pl-uk.t3x"/>
        <file name="pl-uk.t3x.bin"/>
      </program>
      <program name="lt-proc $1">
        <file name="pl-uk.autogen.bin"/>
      </program>
      <program name="lt-proc -p">
        <file name="pl-uk.autopgen.bin"/>
      </program>
    </pipeline>
  </mode>

